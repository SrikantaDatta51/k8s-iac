            "title": "Single-Node Job Failure Signals",
            "description": "Nodes with active GPU health issues, ECC errors, memory pressure, or thermal violations that predict single-node job failures.",
            "type": "table",
            "datasource": {
                "type": "prometheus",
                "uid": "${datasource}"
            },
