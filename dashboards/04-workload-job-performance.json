            "options": {
                "showHeader": true,
                "sortBy": [
                    {
                        "displayName": "Signal",
                        "desc": true
                    }
                ]
            },
            "transformations": [
                {
                    "id": "organize",
                    "options": {
                        "excludeByName": {
                            "Time": true,
                            "__name__": true,
                            "job": true,
                            "cluster": true
                        },
                        "renameByName": {
                            "instance": "Node",
                            "gpu": "GPU",
                            "Value": "Signal"
                        }
                    }
                }
            ],
            "targets": [
                {
                    "refId": "A",
                    "datasource": {
                        "type": "prometheus",
                        "uid": "${datasource}"
                    },
                    "expr": "(gpu_health_overall{node=~\"$node\"} != 0) or (gpu_ecc_dbe_agg{node=~\"$node\"} > 0) or (gpu_thermal_violation{node=~\"$node\"} > 0)",
